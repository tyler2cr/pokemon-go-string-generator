{
  "name": "pokemon-go-string-generator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-gh-pages": "ng build --output-path docs --base-href /pokemon-go-string-generator/",
    "angular-cli-ghpages": "ng deploy --repo=git@github.com:tyler2cr/pokemon-go-string-generator.git --branch=trunk --no-silent",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "github-docs": "cp -r dist/pokemon-go-string-generator/browser docs ",
    "dev:ssr": "ng run pokemon-go-string-generator:serve-ssr",
    "serve:ssr": "node dist/pokemon-go-string-generator/server/main.js",
    "build:ssr": "ng build && ng run pokemon-go-string-generator:server",
    "prerender": "ng run pokemon-go-string-generator:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.4",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.4",
    "@angular/compiler": "^15.0.4",
    "@angular/core": "^15.0.4",
    "@angular/forms": "^15.0.4",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.4",
    "@angular/platform-browser-dynamic": "^15.0.4",
    "@angular/platform-server": "^15.0.4",
    "@angular/router": "^15.0.4",
    "@nguniversal/express-engine": "^15.0.0",
    "express": "^4.15.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~15.0.4",
    "@angular/compiler-cli": "^15.0.4",
    "@nguniversal/builders": "^15.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^14.15.0",
    "angular-cli-ghpages": "^1.0.5",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.4"
  }
}
